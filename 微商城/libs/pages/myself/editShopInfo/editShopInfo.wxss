.callImage {
    width: 22px;
    height: 13px;
    margin-left: 12px;
    margin-top: 7px;
    float: left;
}

.settingLabel {
    color: white;
    text-align: right;
    font-size: 14px;
    margin-right: 12px;
    float: right;
}

.cell {
    width: 100%;
    background-color: white;
    line-height: 44px;
}

.cellIndicator {
    width: 7px;
    height: 13px;
    float: right;
    margin-right: 12px;
    margin-top: 15.5px;
}

.lineSpace {
    width: 100%;
    background-color: #e0e0e0;
    margin-top: -1px;
    float: left;
}

.cellImage {
    margin-left: 12px;
    margin-top: 11.5px;
    float: left;
}

.cellTitle {
    margin-left: 10px;
    font-size: 14px;
    color: #545454;
    float: left;
}

.cellInput {
    height: 100%;
    padding-left: 40rpx;
    box-sizing: border-box;
    font-size: 30rpx;
}

.warn {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    padding: 20rpx 20rpx;
    box-sizing: border-box;
    color: red;
}

.warn-img {
    margin-right: 20rpx;
}

.cell-code {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    border-bottom: 2rpx solid #ccc;
}

.code {
    margin-left: 50rpx;
}

/*弹窗  */
.showmodel{
    position: fixed;
    top:0;
    left: 0;
    z-index: 9999;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 100%;
    font-size: 30rpx;
    background: rgba(0, 0, 0, 0.5)
}
.model-each{
    display: flex;
    justify-content: flex-start;
    align-items: center;
    width: 100%;
    padding: 0 20rpx;
    color: #fff;
    background-color: #ccc; 
}
.model-input{
    margin-left: 14rpx; 
}
.model-code{
    padding-left: 80rpx;
    box-sizing: border-box;
}

.model-btn{
    display: flex;
    justify-content: center;
    align-items: center;
    padding-bottom: 20rpx;
    box-sizing: border-box; 
}

.myaddress {
    display: flex;
    flex-direction: column;
    width:100%;
    height: 500rpx;
    padding-top: 50rpx;
    box-sizing: border-box;
    /* border: 1rpx solid red; */
}

.myaddress-title {
    display: flex;
    justify-content: flex-start;
    padding-left: 20rpx;
    box-sizing: border-box;
}

.myaddress-list {
    margin-top: 20rpx;
    padding: 0 20rpx 0 70rpx;
    box-sizing: border-box;
}

.myaddress-each {
    display: flex;
    flex-direction: column;
    padding-bottom: 20rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #ccc;
}
