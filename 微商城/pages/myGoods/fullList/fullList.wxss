page {
    background: #f4f4f4;
    min-height: 100%;
}
.dis{
  display: flex
}
.container {
    position: relative;
    background: #f4f4f4;
    min-height: 100%;
    padding-top: 30rpx;
}
.coupon-list {
    width: 100%;
    overflow: hidden;
    padding-left: 30rpx;
    padding-right: 30rpx;
}
.lingquan {
    position: fixed;
    left: 0;
    z-index: 100rpx;
    width: 100%;
    /* height: 100%; */
    padding: 0 20rpx;
    /* top: 1120rpx; */
    bottom: 1px;
}

/*优惠活动  */
.fav1{
    font-size: 40rpx;
    text-align: center;
    margin-top:270rpx; 
}
.fav2{
    text-align: center;
    margin-top: 100rpx;
    font-size: 35rpx;
}
.go {
    border: none;
    background: rgba(255, 255, 255, 0.8);
    border-radius: 4rpx;
    line-height: 48rpx;
    color: #b69150;
    font-size: 24rpx;
}

/* 优惠券样式 */

.dollar-list {
    background: #fff;
    margin: 30rpx auto 0;
    border-radius: 10rpx;
    overflow: hidden;
}

.top {
    padding: 50rpx 50rpx 0;
    background: #f66;
    position: relative;
    border-bottom: 2rpx dashed #fff;
}

.money-msg, .status {
    display: inline-block;
}

.money {
    font-size: 80rpx;
}

.status {
    border: 2rpx solid #fff;
    line-height: 60rpx;
    border-radius: 4rpx;
}

.bottom {
    padding: 30rpx;
    line-height: 50rpx;
}